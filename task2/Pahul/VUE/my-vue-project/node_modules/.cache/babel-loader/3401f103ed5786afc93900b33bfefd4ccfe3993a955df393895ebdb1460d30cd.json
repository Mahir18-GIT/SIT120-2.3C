: #f5f7fa;\r\n  box-shadow: 0px 10px 25px rgba(0, 0, 0, 0.1);\r\n}\r\n\r\nh1 {\r\n  text-align: center;\r\n  color: #2c3e50;\r\n  font-weight: 500;\r\n  margin-bottom: 30px;\r\n}\r\n\r\n.input-container {\r\n  display: flex;\r\n  justify-content: space-between;\r\n}\r\n\r\ninput[type=\"text\"] {\r\n  flex: 1;\r\n  padding: 10px;\r\n  border: none;\r\n  border-bottom: 2px solid #2980b9;\r\n  margin-right: 10px;\r\n  background-color: transparent;\r\n  font-size: 18px;\r\n}\r\n\r\nbutton {\r\n  padding: 10px 20px;\r\n  border: none;\r\n  border-radius: 8px;\r\n  background-color: #2980b9;\r\n  color: #ffffff;\r\n  cursor: pointer;\r\n  transition: background-color 0.3s;\r\n}\r\n\r\nbutton:hover {\r\n  background-color: #3498db;\r\n}\r\n\r\nul {\r\n  list-style-type: none;\r\n  padding: 0;\r\n}\r\n\r\n.todo-item {\r\n  display: flex;\r\n  justify-content: space-between;\r\n  align-items: center;\r\n  margin-top: 20px;\r\n  padding: 10px 20px;\r\n  border-radius: 8px;\r\n  background-color: #ecf0f1;\r\n  transition: background-color 0.3s;\r\n}\r\n\r\n.todo-item:hover {\r\n  background-color: #bdc3c7;\r\n}\r\n\r\n.todo-item span.completed {\r\n  text-decoration: line-through;\r\n  color: #7f8c8d;\r\n}\r\n\r\ninput[type=\"checkbox\"] {\r\n  cursor: pointer;\r\n}\r\n\r\n.delete-btn {\r\n  background-color: #e74c3c;\r\n  color: #fff;\r\n  transition: background-color 0.3s;\r\n}\r\n\r\n.delete-btn:hover {\r\n  background-color: #c0392b;\r\n}\r\n</style>\r\n"],"mappings":";AAkBA,eAAe;EACbA,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,OAAO,EAAE,EAAE;MACXC,KAAK,EAAE;IACT,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,OAAOA,CAAA,EAAG;MACR,IAAI,IAAI,CAACH,OAAO,CAACI,IAAI,CAAC,CAAC,EAAE;QACvB,IAAI,CAACH,KAAK,CAACI,IAAI,CAAC;UAAEC,IAAI,EAAE,IAAI,CAACN,OAAO,CAACI,IAAI,CAAC,CAAC;UAAEG,SAAS,EAAE;QAAM,CAAC,CAAC;QAChE,IAAI,CAACP,OAAM,GAAI,EAAE;MACnB;IACF,CAAC;IACDQ,UAAUA,CAACC,KAAK,EAAE;MAChB,IAAI,CAACR,KAAK,CAACS,MAAM,CAACD,KAAK,EAAE,CAAC,CAAC;IAC7B;EACF;AACF"},"metadata":{},"sourceType":"module","externalDe